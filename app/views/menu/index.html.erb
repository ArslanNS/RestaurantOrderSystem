<!-- Navbar -->
<section class="hero is-small is-info is-bold">
  <div class="hero-body">
    <div class="container">
      <h1 class="title">
        Oaxaca Mexican Food
      </h1>
      <h2 class="subtitle">
        Table Ordering System
      </h2>
      <div class="tile is-ancestor">
        <!-- All other tile elements -->
        <div class="tile is-parent">
          <!-- Add content or other tiles -->
          <article class="tile is-child notification is-dark" onclick="location.href='javascript:history.back()';" style="cursor: pointer">
            <p class="title">Return to Menu</p>
          </article>
        </div>
        <div class="tile is-parent">
          <%= form_with model: @call, url: {action: "create"}, id: "call-waiter" do |f| %>
            <%= f.hidden_field :table_id, value: 0 %>
          <% end %>
          <article class="tile is-child notification is-dark" onclick="document.getElementById('call-waiter').submit();" style="cursor: pointer">
            <p class="title">Call Waiter</p>
          </article>
        </div>
        <div class="tile is-parent">
          <article class="tile is-child notification is-dark" onclick="location.href='http://www.google.com';" style="cursor: pointer">
            <p class="title">Checkout <i class="fas fa-shopping-cart" style="float: right;"></i></p>
          </article>
        </div>
      </div>
    </div>
  </div>
</section>

